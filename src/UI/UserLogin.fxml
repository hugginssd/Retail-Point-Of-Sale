<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="236.0" prefWidth="369.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="DAL.UserLoginController">
   <children>
      <TextField fx:id="txtUserName" layoutX="69.0" layoutY="65.0" prefHeight="25.0" prefWidth="232.0" promptText="USERNAME">
         <font>
            <Font name="Humnst777 BT Roman" size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="69.0" layoutY="118.0" prefHeight="25.0" prefWidth="232.0" promptText="PASSWORD">
         <font>
            <Font name="Humnst777 BT Roman" size="13.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnSignIn" layoutX="69.0" layoutY="164.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="91.0" text="SIGN IN">
         <font>
            <Font name="Humanst521 BT Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="210.0" layoutY="164.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="91.0" text="CANCEL">
         <font>
            <Font name="Humanst521 BT Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="152.0" layoutY="25.0" text="SIGN IN" textFill="#3c73bc">
         <font>
            <Font name="Humanst521 BT Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
